<button 
  style="margin-left: 5px;" type="button" class="btn btn-success" data-toggle="modal" data-target="#myModal">
  Set Access Keys
 </button>
 
<button ng-click="refresh()" type="button" class="btn btn-default">
Refresh
 <span ng-show="refreshing">
    <i class="fa fa-spinner fa-spin"></i>
  </span>
</button>
<!-- Modal -->
<div id="myModal" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <form class="form-horizontal" ng-submit="onClickLoginBtn()" role="form">  
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Set Access Keys</h4>
        </div>
        <div class="modal-body">
          <!-- access key id -->
          <div class="form-group">
            <label class="control-label col-sm-4" for="type">Access Key Id</label>
            <div class="col-sm-8">
              <input
                id="accesskey"
                type="text"
                class="form-control"
                name="accesskey"
                ng-model="accessKeysForm.accesskey">
            </div>
          </div>  
          
           <!-- secret access key -->
          <div class="form-group">
            <label class="control-label col-sm-4" for="type">Secret Access Key</label>
            <div class="col-sm-8">
              <input
                id="secretkey"
                type="text"
                class="form-control"
                name="secretkey"
                ng-model="accessKeysForm.secretkey">
            </div>
          </div>  
          
        </div>
        <div class="modal-footer">
          <button 
            id="save"
          type="submit"
          class="btn btn-success"
          ng-click="onClickSaveAccessKeysBtn()"
          data-dismiss="modal"
          style="width: 80px !important;">
          Save
          </button>
        </div>
      </div>
      </form>
  </div>
</div>
<p>
<table id="tickets" class="table table-striped">      
  <tr>
     <th>Stack Name</th>
     <th>Region</th>
     <th>Url</th>
     <th>Healthy Web Servers</th>     
     <th>Avg CPU Usage</th>
     <th>Avg Network Data In/Out</th>     
     <th>Status</th>
  </tr>   
</table>  